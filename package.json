{
  "name": "weather",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "lint-fix": "eslint ./src/**/*.tsx --fix",
    "rules": "yarn tsc",
    "format": "prettier --write ./src",
    "pod": "bash -c \"export USE_FRAMEWORKS=static; export USE_HERMES=0; yarn install; cd ios; npx react-native setup-ios-permissions; pod install --clean-install; cd ..\"",
    "pod-release": "bash -c \"export USE_FRAMEWORKS=static; yarn install; cd ios; npx react-native setup-ios-permissions; pod install --clean-install; cd ..\"",
    "repod": "yarn pod:clear; yarn install; yarn pod"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "reactNativePermissionsIOS": [
    "LocationAccuracy",
    "LocationAlways",
    "LocationWhenInUse"
  ],
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.22.7",
    "axios": "^1.4.0",
    "date-fns": "^2.30.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "mobx": "^6.10.0",
    "mobx-react": "^9.0.0",
    "react": "18.2.0",
    "react-native": "0.72.3",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-permissions": "^3.8.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.9",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "babel-jest": "^29.2.1",
    "eslint": "^8.46.0",
    "eslint-plugin-react": "^7.33.1",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.7",
    "prettier": "^3.0.1",
    "react-native-dotenv": "^3.4.9",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
